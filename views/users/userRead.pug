extends userLayout.pug
block userContent
  div.user-list
    h1= title
    p Welcome to #{title}
    p Users:
    ul.list-u
      each obj in data
        li 
          p 
            span â„–: 
            span= obj.id
          p
            span Name: 
            span= obj.name
          p
            span Age: 
            span= obj.age
          p
            span Good: 
            span= obj.goodOne ? 'Good one' : 'Bad One'
  - var n = 0;
    div.pagination
      while n < pages
        - n++
        a(href='?page='+n)= n